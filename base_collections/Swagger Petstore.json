  "requests": [
    {
      "name": "Update an existing pet",
      "url": "https://petstore.swagger.io/v2/pet",
      "method": "PUT",
      "params": [],
      "body": {
        "json": "Pet object that needs to be added to the store"
      },
      "auth": "no_auth"
    },
