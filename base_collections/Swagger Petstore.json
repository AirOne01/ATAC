      "url": "https://petstore.swagger.io/v2/pet/findByStatus?status={{status}}",
      "method": "GET",
      "params": [
        {
          "enabled": true,
          "data": [
            "status",
            "{{status}}"
          ]
        }
      ],
      "body": {
        "raw": ""
      },
      "auth": "no_auth"
    },
